#include<stdio.h>
int main(){
int nums1[]={2,3,2},nums2[]={1,2};
int n=sizeof(nums1)/sizeof(nums1[0]);
int m=sizeof(nums2)/sizeof(nums2[0]);
int a1=0,a2=0;
for(int i=0;i<n;i++){
for(int j=0;j<m;j++){
if(nums1[i]==nums2[j]){a1++;break;}
}}
for(int i=0;i<m;i++){
for(int j=0;j<n;j++){
if(nums2[i]==nums1[j]){a2++;break;}
}}
printf("[%d,%d]",a1,a2);
return 0;
}

output: 
[2,1]
